#!/usr/bin/env bash

SCRIPTDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null && pwd )"

(cd ${SCRIPTDIR}/..; aws lambda invoke \
  --invocation-type RequestResponse \
  --function-name minimal_lambda_function \
  --region us-west-2 \
  --payload '{"name":"john","qwer":"asdr"}' \
  --log-type Tail /dev/stdout | jq -r .LogResult | base64 -D ; xargs -n 1 curl < urls.txt )
